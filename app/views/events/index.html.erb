<h1>Listing events</h1>

<table>
  <tr>
  
    <th>Primary</th>
    <th>Name</th>
    <th>Date</th>
    <th>Plan</th>
  </tr>

<% @events.each do |events| %>
  <tr>
    <td><%=h events.primary %></td>
    <td><%=h events.name %></td>
    <td><%=h events.date %></td>
    <td><%=h events.plan_id %></td>
    <td><%= link_to 'Show', resource_path(events) %></td>
    <td><%= link_to 'Edit', edit_resource_path(events) %></td>
    <td><%= link_to 'Destroy', resource_path(events), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New events', new_resource_path %>